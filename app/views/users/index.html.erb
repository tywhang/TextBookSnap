<div class="col-sm-10 col-sm-offset-1">
	<table class="table">
		<thead>
			<tr>
				<th>ID</th>
				<th>First Name</th>
				<th>Last Name</th>
				<th>Email</th>
				<th>Password Digest</th>
				<th>Created At</th>
				<th>Updated At</th>
			</tr>
		</thead>
		<tbody>
			<tr>
				<% @users.each do |user| %>
					<td><%= user.id %></td>
					<td><%= user.first_name %></td>
					<td><%= user.last_name %></td>
					<td><%= user.email %></td>
					<td><%= user.password_digest %></td>
					<td><%= user.created_at %></td>
					<td><%= user.updated_at %></td>
				<% end %>
			</tr>
		</tbody>
	</table>
</div>